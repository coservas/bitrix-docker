# Project name
COMPOSE_PROJECT_NAME=bitrix

# PHP version settings. Allowed value is php56 or php7.
PHP_VERSION=php7

# Web server type. Allowed value is apache or nginx.
WEB_SERVER_TYPE=nginx

# MySQL settings
MYSQL_DATABASE=bitrix
MYSQL_USER=bitrix
MYSQL_PASSWORD=bitrix
MYSQL_ROOT_PASSWORD=root

INTERFACE=0.0.0.0

# Site path
SITE_PATH=./site/
